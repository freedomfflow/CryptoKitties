<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Academy kitties </title>
    <script type="text/javascript" src="assets/js/jquery-3.4.1.js"></script>
    <link rel="stylesheet" href="assets/bootstrap/css/bootstrap.min.css">
    <script src="assets/bootstrap/js/popper.js"></script>
    <script src="assets/bootstrap/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="assets/css/mystyle.css">
    <link rel="stylesheet" href="assets/css/kitties.css">
    <link rel="stylesheet" href="assets/css/colors.css">
    <link rel="stylesheet" href="assets/css/factory.css">
    <link rel="stylesheet" href="assets/css/frontend.css">
</head>
<body>
<div class="container p-1" style="margin-top: 12vh;margin-bottom: 10vh;">
    <div align="center">
        <h2 class="c-white">Kitties-Factory</h2>
        <p class="c-white">Create your custom Kitty</p>
    </div>
    <div class="row">
        <div class="col-lg-4 catBox m-2 light-b-shadow">
            <div class="cat_body">
                <div class="cat_body_inner">
                </div>
                <div class="cat">
                    <div class="cat_ears">
                        <div class="cat_ears_ear cat_ears_ear_l">
                        </div>
                        <div class="cat_ears_ear cat_ears_ear_r">
                        </div>
                    </div>
                    <div class="cat_whiskers">
                        <div class="cat_whiskers_whisker cat_whiskers_whisker_l">
                            <span></span>
                        </div>
                        <div class="cat_whiskers_whisker cat_whiskers_whisker_r">
                            <span></span>
                        </div>
                    </div>
                    <div class="cat_face">
                        <div class="cat_face_pattern cat_face_pattern_t">
                        </div>
                        <div class="cat_face_pattern cat_face_pattern_l">
                        </div>
                        <div class="cat_face_pattern cat_face_pattern_r">
                        </div>
                        <div class="cat_eyes">
                            <div class="cat_eyes_eye cat_eyes_eye_l">
                                <div class="cat_eyes_eyelid cat_eyes_eyelid_l"></div>
                                <span></span>
                            </div>
                            <div class="cat_eyes_eye cat_eyes_eye_r">
                                <div class="cat_eyes_eyelid cat_eyes_eyelid_r"></div>
                                <span></span>
                            </div>
                        </div>
                        <div class="cat_nose">
                        </div>
                        <div class="cat_mouth">
                        </div>
                    </div>
                    <div class="cat_hands">
                        <div class="cat_hands_hand cat_hands_hand_l">
                        </div>
                        <div class="cat_hands_hand cat_hands_hand_r">
                        </div>
                    </div>

                    <div class="cat_legs">
                        <div class="cat_legs_leg cat_legs_leg_l">
                        </div>
                        <div class="cat_legs_leg cat_legs_leg_r">
                        </div>
                    </div>
                    <div class="cat_tail">
                        <div class="cat_tail_inner">
                            <span class="cat_tail_pattern cat_tail_pattern_1"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_2"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_3"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_4"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_5"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_6"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_7"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_8"></span>
                            <span class="cat_tail_pattern cat_tail_pattern_9"></span>
                        </div>
                    </div>
                </div>
            </div>
            <br>
            <div class="dnaDiv" id="catDNA">
                <b>
                    DNA:
                    <!-- Colors -->
                    <span id="dnabody"></span>
                    <span id="dnamouth"></span>
                    <span id="dnaeyes"></span>
                    <span id="dnaears"></span>

                    <!-- Cattributes -->
                    <span id="dnashape"></span>
                    <span id="dnadecoration"></span>
                    <span id="dnadecorationMid"></span>
                    <span id="dnadecorationSides"></span>
                    <span id="dnadanimation"></span>
                    <span id="dnaspecial"></span>
                </b>
            </div>
        </div>
        <div class="col-lg-7 cattributes m-2 light-b-shadow">
            <div class="row">
                <div class="col-lg-6">
                    <!-- Cat colors -->
                    <div id="catColors">
                        <div class="form-group">
                            <label for="formControlRange"><b>Head and body color</b><span class="badge badge-dark ml-2" id="headcode"></span></label>
                            <input type="range" min="10" max="98" class="form-control-range" id="bodycolor">
                        </div>
                    </div>
                    <div id="facePartColors">
                        <div class="form-group">
                            <label for="formControlRange"><b>Facial Feature color</b><span class="badge badge-dark ml-2" id="mouthcode"></span></label>
                            <input type="range" min="10" max="98" class="form-control-range" id="facialfeaturecolor">
                        </div>
                    </div>
                    <div id="eyeColors">
                        <div class="form-group">
                            <label for="formControlRange"><b>Eye Color</b><span class="badge badge-dark ml-2" id="eyecode"></span></label>
                            <input type="range" min="10" max="98" class="form-control-range" id="eyecolor">
                        </div>
                    </div>
                    <div id="pawEarColors">
                        <div class="form-group">
                            <label for="formControlRange"><b>Paw & Marking Color</b><span class="badge badge-dark ml-2" id="pawcode"></span></label>
                            <input type="range" min="10" max="98" class="form-control-range" id="pawcolor">
                        </div>
                    </div>
                </div>
                <div class=col-lg-6">
                    <div id="eyeShape">
                        <div class="form-group">
                            <label for="formControlRange"><b>Eye Shape</b><span class="badge badge-dark ml-2" id="eyeshapecode"></span></label>
                            <input type="range" min="0" max="8" class="form-control-range" id="eyeshape">
                        </div>
                    </div>
                    <div id="decorationTop">
                        <div class="form-group">
                            <label for="formControlRange"><b>Head Decoration</b><span class="badge badge-dark ml-2" id="headdecorationcode"></span></label>
                            <input type="range" min="0" max="45" class="form-control-range" id="headdecoration">
                        </div>
                    </div>
                    <div id="decorationSides">
                        <div class="form-group">
                            <label for="formControlRange"><b>Face Side Decorations</b><span class="badge badge-dark ml-2" id="facesidedecorationcode"></span></label>
                            <input type="range" min="0" max="20" class="form-control-range" id="facesidedecoration">
                        </div>
                    </div>
                    <div id="decorationTail">
                        <div class="form-group">
                            <label for="formControlRange"><b>Tail Decorations</b><span class="badge badge-dark ml-2" id="taildecorationcode"></span></label>
                            <input type="range" min="1" max="10" class="form-control-range" id="taildecoration">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br>
</div>
<footer style="align: left;">
    <p>Crypto Kitties Development Project September 2020</p>
</footer>

</body>
<script src="assets/js/colors.js"></script>
<script src="assets/js/catSettings.js"></script>
<script src="assets/js/catFactory.js"></script>

</html>
